        "# 성적 리스트\n",
        "성적 = [85, 92, 78, 90, 87]\n",
        "\n",
        "# 총합을 저장할 변수\n",
        "총합 = 0\n",
        "\n",
        "# 반복문을 이용해서 총합 계산\n",
        "for 점수 in 성적:\n",
        "    총합 += 점수\n",
        "\n",
        "# 평균 계산\n",
        "평균 = 총합 / len(성적)\n",
        "\n",
        "# 결과 출력\n",
        "print(f\"평균 성적은 {평균:.1f}점입니다.\")\n"
      ],
      "metadata": {
